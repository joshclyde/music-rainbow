{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "no-unused-variable": true,
    "jsx-no-multiline-js": false,
    "max-line-length": [true, 120],
    "object-literal-sort-keys": false,
    "jsx-key": false,
    "no-any": true,
    "jsx-no-lambda": false,
    "no-empty-interface": false,
    "member-access": false,
    "no-console": false
  }
}
